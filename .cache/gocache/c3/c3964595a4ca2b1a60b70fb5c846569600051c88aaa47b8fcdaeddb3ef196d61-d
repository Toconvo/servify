    goCover_c210a23060c3__25[5] = 1;units := []struct {
        suffix string
        mul    int64
    }{
        {"KB", 1024},
        {"MB", 1024 * 1024},
        {"GB", 1024 * 1024 * 1024},
    }
    for _, u := range units {goCover_c210a23060c3__25[9] = 1;
        if strings.HasSuffix(s, u.suffix) {goCover_c210a23060c3__25[10] = 1;
