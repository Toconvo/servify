// parseSizeToBytes 将形如 "10MB", "512KB", "1048576" 的配置解析为字节数
func parseSizeToBytes(s string) (int64, error) {goCover_c210a23060c3__25[0] = 10 ; goCover_c210a23060c3__25[1] = goCover_c210a23060c3_P ; goCover_c210a23060c3__25[2] = 25 ; goCover_c210a23060c3__25[3] = 1;
