    if s == "" {goCover_c210a23060c3__25[7] = 1;
        return 0, nil
    }
    // 纯数字直接解析为字节
    goCover_c210a23060c3__25[4] = 1;if n, err := strconv.ParseInt(s, 10, 64); err == nil {goCover_c210a23060c3__25[8] = 1;
