// isAllowedType 判断给定文件是否满足允许类型（支持 MIME 与扩展名）
func isAllowedType(filename, mimeType string, allowed []string) bool {goCover_c210a23060c3__26[0] = 10 ; goCover_c210a23060c3__26[1] = goCover_c210a23060c3_P ; goCover_c210a23060c3__26[2] = 26 ; goCover_c210a23060c3__26[3] = 1;
    // 允许类型若包含通配符 "*" 或 "*/*"，直接通过
    for _, t := range allowed {goCover_c210a23060c3__26[6] = 1;
        if t == "*" || t == "*/*" {goCover_c210a23060c3__26[7] = 1;
            return true
        }
    }
    goCover_c210a23060c3__26[4] = 1;ext := strings.ToLower(filepath.Ext(filename))
    for _, t := range allowed {goCover_c210a23060c3__26[8] = 1;
        t = strings.ToLower(strings.TrimSpace(t))
        if strings.HasPrefix(t, ".") {goCover_c210a23060c3__26[9] = 1;
            if ext == t {goCover_c210a23060c3__26[10] = 1;
                return true
            }
        } else{ goCover_c210a23060c3__26[11] = 1;{
            // 作为 MIME 类型匹配（支持前缀，如 "image/"）
            if mimeType == t || (strings.HasSuffix(t, "/*") && strings.HasPrefix(mimeType, strings.TrimSuffix(t, "*"))) {goCover_c210a23060c3__26[12] = 1;
                return true
            }
        }}
    }
    goCover_c210a23060c3__26[5] = 1;return false
}

